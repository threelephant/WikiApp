@model List<StatsModel>

<h2 class="mb-3">Статистика пользователей за @ViewBag.Date</h2>

<table class="table">
    <tr>
        <th scope="col">Пользователь</th>
        <th scope="col">Год</th>
        <th scope="col">Месяц</th>
        <th scope="col">Заявление на правки</th>
        <th scope="col">Модератор</th>
        <th scope="col">Принятые правки</th>
    </tr>
@foreach (var stats in Model)
{
    <tr>
        <td>@stats.User</td>
        <td>@stats.Year</td>
        <td>@stats.Month</td>
        <td>@stats.Requests</td>
        <td>@stats.Moderators</td>
        <td>@stats.AcceptedRequests</td>
    </tr>
}
</table>